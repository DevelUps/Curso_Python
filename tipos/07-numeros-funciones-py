import math


print(round(1.4))  # redondea al numero mas cercano
print(round(2.5))  # redondea al numero mas cercano
print(round(3.6))  # redondea al numero mas cercano
print(round(4.7))  # redondea al numero mas cercano
print("abs")
print(abs(-77))
print(abs(55))

print("Math")

print(math.ceil(1.1))  # lo lleva al entero mas cercano acendente
print(math.floor(1.999999))  # lo lleva al entero mas cercano decendente
# print(math.isnan(23))  # lo lleva al entero mas cercano decendente

print(math.pow(10, 3))
print(math.sqrt(9))
